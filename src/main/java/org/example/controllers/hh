 public Map<String, Object> MatriceSimilarite(ArrayList<String> cellValues) {
        Map<String, Object> result = new HashMap<>();

        try {
            Workbook workbook = new XSSFWorkbook();
            Sheet sheet = workbook.createSheet("Cell Values");

            int rowCount = 0;
            for (String value : cellValues) {
                Row row = sheet.createRow(rowCount++);
                Cell cell = row.createCell(0);
                cell.setCellValue(value);
            }

            // Generate a unique file name
            String fileName = "cell_values_" + System.currentTimeMillis() + ".xlsx";
            String filePath = "C:/Users/youss/Desktop/backCAH/src/main/java/org/example/FilesData" + fileName;

            // Write the workbook to the file system
            FileOutputStream outputStream = new FileOutputStream(filePath);
            workbook.write(outputStream);
            workbook.close();
            outputStream.close();
            Cls.setVilles(cellValues);
            matrice=_cah.MatriceDistance(cellValues);
            Map<String, Object> result = new HashMap<>();
            result.put("matrice", matrice);
            result.put("cellValues", cellValues);
            result.put("filePath", filePath); // Add the file path to the result map
        } catch (IOException e) {
            e.printStackTrace();
            // Handle the file processing error
        }


        return result;
    }